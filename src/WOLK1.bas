10 REM ***BANEN VAN DYNAMISCH SYSTEEM***
20 REM ***NAAM:WOLK1 (C) 1987 HANS LAUWERIER***
30 SCREEN 3: CLS
40 DIM X(8), Y(8), P(8)
50 WINDOW (-40, -30)-(40, 30)
60 A = -.5: B = 2: REM ***WAARDEN VAN PARAMETERS***
70 DATA 2,0,200,4,0,400,6,0,600,8,0,800
80 DATA 10,0,1000,12,0,1200,14,0,1400,16,0,1600
90 FOR I = 1 TO 8: READ X(I), Y(I), P(I): NEXT I
100 FOR K = 1 TO 8: X = X(K): Y = Y(K): P = P(K)
110 GOSUB 140: FOR N = 0 TO P: PSET (X, Y)
120 Z = X: X = Y + W: GOSUB 140: Y = W - Z
130 NEXT N: NEXT K: END
140 W = X * (A + B / (1 + ABS(X))): RETURN: END

